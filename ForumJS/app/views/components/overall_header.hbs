<!DOCTYPE html>
<html dir="{S_CONTENT_DIRECTION}" lang="{S_USER_LANG}">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    {META}
    <title>{{#if UNREAD_NOTIFICATIONS_COUNT }} ({{ UNREAD_NOTIFICATIONS_COUNT }}) {{/if}} {{#and  (not S_VIEWFORUM) ( not S_VIEWTOPIC ) }} {{SITENAME}} - {{/and}}{{#if S_IN_MCP}}{{L_MCP}} - {{else}}{{#if S_IN_UCP }}{{L_UCP}} - {{/if}}{{/if}}{{PAGE_TITLE}}{{#or S_VIEWTOPIC S_VIEWFORUM }} - {{SITENAME}}{{/or}}</title>

	
    {{#if U_CANONICAL }}
		<link rel="canonical" href="{U_CANONICAL}">
	{{/if}}

	<link href="css/theme/base.css" rel="stylesheet">
	<link href="css/theme/bidi.css" rel="stylesheet">
	<link href="css/theme/buttons.css" rel="stylesheet">
	<link href="css/theme/colours.css" rel="stylesheet">
	<link href="css/theme/common.css" rel="stylesheet">
	<link href="css/theme/content.css" rel="stylesheet">
	<link href="css/theme/cp.css" rel="stylesheet">
	<link href="css/theme/forms.css" rel="stylesheet">
	<link href="css/theme/icons.css" rel="stylesheet">
	<link href="css/theme/common.css" rel="stylesheet">
	<link href="css/theme/links.css" rel="stylesheet">
	<link href="css/theme/normalize.css" rel="stylesheet">
	<link href="css/theme/plupload.css" rel="stylesheet">
	<link href="css/theme/print.css" rel="stylesheet">
	<link href="css/theme/responsive.css" rel="stylesheet">
	<link href="css/theme/stylesheet.css" rel="stylesheet">
	<link href="css/theme/tweaks.css" rel="stylesheet">
	<link href="css/theme/utilities.css" rel="stylesheet">
    <link href="css/font-awesome.css" rel="stylesheet">

</head>
<body id="phpbb" class="nojs notouch section-{{SCRIPT_NAME}} {{S_CONTENT_DIRECTION}} {{BODY_CLASS}}">
<!-- EVENT overall_header_body_before -->
<div id="wrap" class="wrap">
	<a id="top" class="top-anchor" accesskey="t"></a>
	<div id="page-header">
		<div class="headerbar" role="banner">
		<!-- EVENT overall_header_headerbar_before -->
			<div class="inner">
			<div id="site-description" class="site-description">
				<a id="logo" class="logo" href="{{#if U_SITE_HOME }}{{U_SITE_HOME}}{{else}}{{U_INDEX}}{{/if}}" title="{{#if U_SITE_HOME }}{{i18n "L_SITE_HOME"}}{{else}}{{i18n "L_INDEX"}}{{/if}}"><span class="site_logo"></span></a>
				<h1>{{SITENAME}}</h1>
				<p>{{SITE_DESCRIPTION}}</p>
				<p class="skiplink"><a href="#start_here">{{ i18n "L_SKIP" }}</a></p>
			</div>

            {{#and  S_DISPLAY_SEARCH ( not S_IN_SEARCH ) }}
			<div id="search-box" class="search-box search-header" role="search">
				<form action="{{U_SEARCH}}" method="get" id="search">
				<fieldset>
					<input name="keywords" id="keywords" type="search" maxlength="128" title="{{i18n "L_SEARCH_KEYWORDS"}}" class="inputbox search tiny" size="20" value="{{SEARCH_WORDS}}" placeholder="{{i18n "L_SEARCH_MINI"}}" />
					<button class="button button-search" type="submit" title="{{i18n "L_SEARCH"}}">
						<i class="icon fa-search fa-fw" aria-hidden="true"></i><span class="sr-only">{{i18n "L_SEARCH"}}</span>
					</button>
					<a href="{{U_SEARCH}}" class="button button-search-end" title="{{i18n "L_SEARCH_ADV"}}">
						<i class="icon fa-cog fa-fw" aria-hidden="true"></i><span class="sr-only">{{i18n "L_SEARCH_ADV"}}</span>
					</a>
					{{S_SEARCH_HIDDEN_FIELDS}}
				</fieldset>
				</form>
			</div>
			{{/and}}

			</div>

		</div>

        {{> navbar_header }}

	</div>

	<a id="start_here" class="anchor"></a>
	<div {{#and S_BOARD_DISABLED S_USER_LOGGED_IN }} <!-- and (U_MCP or U_ACP) -->
		<div id="information" class="rules">
			<div class="inner">
				<strong>{{i18n "L_INFORMATION"}}{{i18n "L_COLON"}}</strong> {{i18n "L_BOARD_DISABLED"}}
			</div>
		</div>
		{{/and}}

